<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image" />
    </swiper-item>
  </block>
</swiper>
 
<view class="main">
  <view class="view-search">
    <input class="input" placeholder-class="input-placeholder" placeholder="搜索从这里开始" bindinput="bindSearchInput" />
    <image class="button" src="../../res/img/global_search.png" bindtap="tapSearch" />
  </view>
  <view class="request_null">
    <text hidden="{{nullHidden}}">没有搜索结果，搜索“阿米哥”试试？</text>
  </view>
  <loading hidden="{{loadingHidden}}">{{lodingInfo}}</loading>
</view>

<map id="map" longitude="114.0316650000" latitude="22.5433410000" scale="12" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange" show-location style="width: 100%; height: 300px;"></map>