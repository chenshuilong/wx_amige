<view class="main">
  <view class="view-search">
    <input class="input" placeholder-class="input-placeholder" placeholder="搜索从这里开始" bindinput="bindSearchInput" />
    <image class="button" src="../../res/img/global_search.png" bindtap="tapSearch" />
  </view>
  <view class="request_null">
    <text hidden="{{nullHidden}}">没有搜索结果，搜索“阿米哥”试试？</text>
  </view>
  <loading hidden="{{loadingHidden}}">{{lodingInfo}}</loading>
</view>